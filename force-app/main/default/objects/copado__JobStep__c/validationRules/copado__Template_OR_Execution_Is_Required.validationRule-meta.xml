<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>copado__Template_OR_Execution_Is_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK( copado__JobTemplate__c ), ISBLANK(  copado__JobExecution__c  ) )</errorConditionFormula>
    <errorMessage>JobTemplate or Execution is required</errorMessage>
</ValidationRule>
